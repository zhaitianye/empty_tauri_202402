{
  "package": {
    "productName": "bt-baidu",
    "version": "0.0.0"
  },
  "tauri": {
    "allowlist": {
      "all": false,
      "shell": {
        "all": false,
        "open": true
      }
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "百度批量翻译",
        "minWidth": 1440,
        "minHeight": 900,
        "width": 1920,
        "height": 1080,
        "url": "https://blog.zhaitianye.com/space/dobot_tools/index.html#/translate",
        "additionalBrowserArgs": "--disable-web-security"
      }
    ],
    "security": {
      "csp": null
    },
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.zhaitianye.bt-baidu",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    }
  }
}
